#: main.c:338
msgid "Options:\n"
msgstr "Tùy chọn:\n"

#: main.c:339
msgid "  -b, -m                      Ignored for compatibility.\n"
msgstr "  -b, -m                      Bỏ qua để tương thích.\n"

